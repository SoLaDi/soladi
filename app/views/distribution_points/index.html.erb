<p id="notice"><%= notice %></p>

<h1>Distribution Points</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Street</th>
      <th>Housenumber</th>
      <th>Zipcode</th>
      <th>City</th>
      <th>Contact name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @distribution_points.each do |distribution_point| %>
      <tr>
        <td><%= distribution_point.name %></td>
        <td><%= distribution_point.street %></td>
        <td><%= distribution_point.housenumber %></td>
        <td><%= distribution_point.zipcode %></td>
        <td><%= distribution_point.city %></td>
        <td><%= distribution_point.contact_name %></td>
        <td><%= link_to 'Show', distribution_point %></td>
        <td><%= link_to 'Edit', edit_distribution_point_path(distribution_point) %></td>
        <td><%= link_to 'Destroy', distribution_point, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Distribution Point', new_distribution_point_path %>
